net: "ECO_Lite.prototxt"

# testing parameter
test_iter: 1530
test_interval: 500
test_initialization: false

# output
display: 20
average_loss: 20
snapshot: 500
snapshot_prefix: "snapshots/ECO_Lite_HMDB51"
debug_info: false

# learning rate
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 1500
max_iter: 5000
iter_size: 2

# parameter of SGD
momentum: 0.9
weight_decay: 0.0005
clip_gradients: 40

# GPU setting
solver_mode: GPU
solver_type: NESTEROV 
richness: 100

