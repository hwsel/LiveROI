name: "C3D"
mem_param {
  optimize_test: true
  optimize_train: true
}
########################################## data ###############################################
layer {
  name: "data"
  type: "VideoData"
  top: "data"
  top: "label"
  video_data_param {
    source: "/misc/lmbraid19/zolfagha/action_recognition/kinetics_newArch/scripts_eval/UCF101/list/scratch/ucf101_split1Shuffled_train.txt"
    batch_size: 32
    new_width: 170
    new_height: 128
    new_length: 16
    num_segments: 1
    modality: RGB
    length_first: true
    name_pattern: "img_%05d.jpg"
    shuffle: true
  }
  transform_param {
    crop_size: 112
    mirror: true
    multi_scale: true
    max_distort: 1
    scale_ratios: [1, 0.875, 0.75, 0.66]
    mean_value: [104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104]
    mean_value: [117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117]
    mean_value: [123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123]
  }
  include: { phase: TRAIN }
}
layer {
  name: "data"
  type: "VideoData"
  top: "data"
  top: "label"
  video_data_param {
    source: "/misc/lmbraid19/zolfagha/action_recognition/kinetics_newArch/scripts_eval/UCF101/list/scratch/ucf101_RGB_split1_test_eval.txt"
    batch_size: 1
    new_width: 170
    new_height: 128
    new_length: 16
    num_segments: 1
    modality: RGB
    name_pattern: "img_%05d.jpg"
    length_first: true
  }
  transform_param {
    crop_size: 112
    mirror: false
    mean_value: [104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104]
    mean_value: [117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117]
    mean_value: [123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123]
  }
  include: { phase: TEST }
}
layer {
  name: "data_reshape"
  type: "Reshape"
  bottom: "data"
  top: "data_reshape"
  reshape_param{
    shape { dim: -1 dim: 3 dim: 16 dim: 112 dim: 112 } 
  }
}
########################################### conv1 ########################################
layer {
  name: "conv1"
  type: "Convolution"
  bottom: "data_reshape"
  top: "conv1"
